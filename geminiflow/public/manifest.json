{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "GeminiFlow",
    "action":{
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "32" : "icon.png"
    }
}